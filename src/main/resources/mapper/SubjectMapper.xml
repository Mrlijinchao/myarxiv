<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myarxiv.myarxiv.mapper.SubjectMapper">

    <resultMap id="BaseResultMap" type="com.myarxiv.myarxiv.domain.Subject">
            <id property="subjectId" column="subject_id" jdbcType="INTEGER"/>
            <result property="subjectName" column="subject_name" jdbcType="VARCHAR"/>
            <result property="subjectDescription" column="subject_description" jdbcType="VARCHAR"/>
            <result property="subjectCreateTime" column="subject_create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        subject_id,subject_name,subject_description,
        subject_create_time
    </sql>
</mapper>
